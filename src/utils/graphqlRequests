query Currentuser {
  currentUser {
    email
  }
}

query ProgramsUser {
  programsUser {
    name
    _id
    _userId
    active
    days {
      day
      isCollapsed
      isDayOff
      exercises {
        muscleGroup
	    	recoveryTime
	    	exercise {
	      	name
	      	equipment
	    	}
	    	sets {
	      	reps
	      	weight
	    	}
      }
    }
  }
}

query HistoryDateUser {
  historyDateUser {
    _id
    _userId
    timestamp
    exercises {
      muscleGroup
      recoveryTime
      exercise {
        name
        equipment
      }
      sets {
        reps
        weight
      }
    }
  }
}

mutation Login {
  login(input: {email: "test@test.test", password: "test"}) {
    _id
    email
    jwt
  }
}

mutation Signup {
  signup(input: {email: "test@test.test", password: "test"}) {
    _id
    email
    jwt
  }
}

mutation Update {
  updateUser(input: {firstName: "jean", lastName: "claude"}) {
    _id
    email
    jwt
  }
}

