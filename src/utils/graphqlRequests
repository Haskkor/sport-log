query currentuser {
  currentUser {
    email
  }
}

query programsUser {
  programsUser {
    name
    _id
    _userId
    active
    days {
      day
      isCollapsed
      isDayOff
      exercises {
        muscleGroup
	recoveryTime
	exercise {
	  name
	  equipment
	}
	sets {
	  reps
	  weight
	}
      }
    }
  }
}

mutation login {
  login(input: {email: "test@test.test", password: "test"}) {
    _id
    email
    jwt
  }
}

mutation signup {
  signup(input: {email: "test@test.test", password: "test"}) {
    _id
    email
    jwt
  }
}

mutation update {
  updateUser(input: {firstName: "jean", lastName: "claude"}) {
    _id
    email
    jwt
  }
}

